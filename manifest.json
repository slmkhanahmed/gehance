{
  "manifest_version": 2,
  "name": "Gehance",
  "description": "Improve Github Starred Repo Experience",
  "version": "1.0",
  "icons": {
    "64": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/stars/*/lists/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": {
      "64": "icons/icon.png"
    },
    "default_popup": "browserAction/index.html",
    "default_title": "Gehance"
  }
}